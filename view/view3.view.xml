<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="xyz.x.controller.view3" xmlns:wt="xyz.x.control">
	<Page title="view3">
		<content>
			<ObjectHeader title="{supName}" intro="{supCity}" id="objHead">
				<attributes>
					<ObjectAttribute title="{i18n>contact}" text="{supContactInfo}"></ObjectAttribute>
					<ObjectAttribute title="{i18n>since}" text="{Since}"></ObjectAttribute>
				</attributes>
			</ObjectHeader>
			<wt:ProductRating id="rating" class="sapUiSmallMarginBeginEnd" change=".onRatingChange"/>
		</content>
	</Page>
</mvc:View>
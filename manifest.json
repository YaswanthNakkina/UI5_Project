{
	"_version": "1.12.0",
	"sap.app": {
		"id": "xyz.x",
		"_version": "1.3.0",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "semObj1",
					"action": "Display"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"_version": "1.3.0",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"flexEnabled": false,
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "xyz.x.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockData/fruit.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"rootView": {
			"viewName": "xyz.x.view.App",
			"id": "idApp",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "xyz.x.view",
				"controlId": "idSplitApp"
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": [
						"fruits"
					]
				},
				{
					"pattern": "FruitDetails/{fruitsId}",
					"name": "detail",
					"target": [
						"fruits",
						"fruitsDetails"
					]
				},
				{
					"pattern": "supdetails/{supId}",
					"name": "supDetail",
					"target": [
						"supDetails"
					]
				}
			],
			"targets": {
				"fruits": {
					"viewName": "view1",
					"controlAggregation": "masterPages",
					"viewId": "idView1",
					"viewLevel": 1
				},
				"fruitsDetails": {
					"viewType": "XML",
					"clearControlAggregation": true,
					"transition": "slide",
					"viewName": "view2",
					"controlAggregation": "detailPages",
					"viewId": "idView2",
					"viewLevel": 2
				},
				"supDetails": {
					"viewName": "view3",
					"viewId": "idView3",
					"viewLevel": 3,
					"controlAggregation": "detailPages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "",
		"_version": "1.1.0"
	}
}